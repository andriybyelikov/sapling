t_gobble__blank -> [ \t\r\n]+;
t_word          -> [a-z]+;

start -> vegetables;
vegetables -> vegetable vegetables | vegetable;
vegetable -> t_word
<
    (if (strequ (lexeme "t_word") "col")
        (emit_line "kapusta")
    (if (strequ (lexeme "t_word") "patata")
        (emit_line "kartofel")
    (if (strequ (lexeme "t_word") "tomatiga")
        (emit_line "pomidor")
        (nil)
    )))
>;
