t_gobble__blank       -> [ \t\r\n]+;
t_gobble__linecomment -> "--".*"\n";
t_lpar                -> "(";
t_rpar                -> ")";
t_symbol              -> [a-z][a-z0-9_]*;
t_string              -> "\""(.|"\\".)*"\"";
t_integer             -> [0-9]+;
t_hexadecimal         -> "0x"[0-9A-Fa-f]+;

start -> lists;
lists -> list lists | list;
list  -> t_lpar args t_rpar;
args  -> arg args | arg;
arg   -> t_symbol | t_string | t_integer | t_hexadecimal | lists;
