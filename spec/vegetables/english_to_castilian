t_gobble__blank -> [ \t\r\n]+;
t_word          -> [a-z]+;

start -> vegetables;
vegetables -> vegetable vegetables | vegetable;
vegetable -> t_word
<
    (if (strequ (lexeme "t_word") "cabbage")
        (emit_line "col")
    (if (strequ (lexeme "t_word") "potato")
        (emit_line "patata")
    (if (strequ (lexeme "t_word") "tomato")
        (emit_line "tomate")
        (nil)
    )))
>;
