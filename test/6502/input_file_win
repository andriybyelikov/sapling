--
-- This version reserves an extra byte of the carriage return in the
-- HELLO SAPLING string.
--

-- iNES Header
-- https://wiki.nesdev.com/w/index.php/INES

fragment ../spec/sample/hex_to_bin
<
    0x4E 0x45 0x53 0x1A 0x01 0x01 0x00 0x00
    0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
>


-- NES init code
-- https://wiki.nesdev.com/w/index.php/Init_code

fragment ../spec/sample/6502_to_bin
<
    SEI          ; PC = 0xC000
    CLD          ; PC = 0xC001
    LDX #0x40    ; PC = 0xC002
    STX 64 0x17  ; PC = 0xC004

    ; can't emit 0xFF due to sapling bug
    LDX #0xFE    ; PC = 0xC007
    INX          ; PC = 0xC009
    
    TXS          ; PC = 0xC00A
    INX          ; PC = 0xC00B
    STX 32 0x00  ; PC = 0xC00C
    STX 32 0x01  ; PC = 0xC00F
    STX 64 0x10  ; PC = 0xC012

; vblankwait1:
    BIT 32 0x02  ; PC = 0xC015
    BPL 0xFB     ; PC = 0xC018 // diff = PC - PC_AFTER = 0xC015 - 0xC01C = 0xFB
    TXA          ; PC = 0xC01A

; clrmem:
    STA 0x00,x   ; PC = 0xC01B
    STA 1 0x00,x ; PC = 0xC01D
    STA 2 0x00,x ; PC = 0xC020
    STA 3 0x00,x ; PC = 0xC023
    STA 4 0x00,x ; PC = 0xC026
    STA 5 0x00,x ; PC = 0xC029
    STA 6 0x00,x ; PC = 0xC02C
    STA 7 0x00,x ; PC = 0xC02F
    INX          ; PC = 0xC032
    BNE 0xE6     ; PC = 0xC033 // diff = PC - PC_AFTER = 0xC01B - 0xC035 = 0xE6

; vblankwait2:
    BIT 32 0x02  ; PC = 0xC035
    BPL 0xFB     ; PC = 0xC038 // diff = PC - PC_AFTER = 0xC035 - 0xC03A = 0xFB
                 ; PC = 0xC03A

; end of init code


;
; Load palette
;

    ; reset $2006 flip-flop
    BIT 32 0x06  ; PC = 0xC03A
    ; set $2006 flip-flop address to palette vram
    LDA #0x3F    ; PC = 0xC03D
    STA 32 0x06  ; PC = 0xC03F
    LDA #0x00    ; PC = 0xC042
    STA 32 0x06  ; PC = 0xC044

    LDA #0x02    ; PC = 0xC047
    STA 32 0x07  ; PC = 0xC049
    LDA #0x30    ; PC = 0xC04C
    STA 32 0x07  ; PC = 0xC04E

;
; Write sample text
;

    ; reset $2006 flip-flop
    BIT 32 0x06  ; PC = 0xC051
    ; set $2006 flip-flop address to name table #0 vram
    LDA #0x20    ; PC = 0xC054
    STA 32 0x06  ; PC = 0xC056
    LDA #0x42    ; PC = 0xC059
    STA 32 0x06  ; PC = 0xC05B

    LDX #0x00    ; PC = 0xC05E
; loop: // PC = 0xC060
    LDA 192 0x82,x ; PC = 0xC060
    BEQ 0x07     ; PC = 0xC063 // diff = PC - PC_AFTER = 0xC06C - 0xC065 = 0x07
    STA 32 0x07  ; PC = 0xC065
    INX          ; PC = 0xC068
    JMP 192 0x60 ; PC = 0xC069
; exitloop: // PC = 0xC06C

;
; reset scroll registers
;

    LDA #0x00    ; PC = 0xC06C
    STA 32 0x05  ; PC = 0xC06E
    STA 32 0x05  ; PC = 0xC071

;
; Enable rendering
;

    LDA #0x88    ; PC = 0xC074
    STA 32 0x00  ; PC = 0xC076
    LDA #0x1E    ; PC = 0xC079
    STA 32 0x01  ; PC = 0xC07B


; loop:
    JMP 192 0x7E ; PC = 0xC07E
; nmi:
; irq:
    RTI          ; PC = 0xC081
                 ; PC = 0xC082
>

-- text, address = 0xC082
fragment ../spec/sample/uppercase_text_to_bin
<
HELLO SAPLING\0
>

