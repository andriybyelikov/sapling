-- iNES Header
-- https://wiki.nesdev.com/w/index.php/INES

fragment ../spec/sample/hex_to_bin
<
    0x4E 0x45 0x53 0x1A 0x01 0x01 0x00 0x00
    0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
>


-- NES init code
-- https://wiki.nesdev.com/w/index.php/Init_code

fragment ../spec/sample/6502_to_bin
<
    .org 0xC000

    SEI
    CLD
    LDX #0x40
    STX 0x40 0x17
    LDX #0xFF
    TXS
    INX
    STX 0x20 0x00
    STX 0x20 0x01
    STX 0x40 0x10

:vblankwait1
    BIT 0x20 0x02
    BPL $:vblankwait1
    TXA

:clrmem
    STA 0x00,x
    STA 0x01 0x00,x
    STA 0x02 0x00,x
    STA 0x03 0x00,x
    STA 0x04 0x00,x
    STA 0x05 0x00,x
    STA 0x06 0x00,x
    STA 0x07 0x00,x
    INX
    BNE $:clrmem

:vblankwait2
    BIT 0x20 0x02
    BPL $:vblankwait2


; end of init code


;
; Load palette
;

    ; reset $2006 flip-flop
    BIT 0x20 0x06
    ; set $2006 flip-flop address to palette vram
    LDA #0x3F
    STA 0x20 0x06
    LDA #0x00
    STA 0x20 0x06

    LDA #0x02
    STA 0x20 0x07
    LDA #0x30
    STA 0x20 0x07

;
; Write sample text
;

    ; reset $2006 flip-flop
    BIT 0x20 0x06
    ; set $2006 flip-flop address to name table #0 vram
    LDA #0x20
    STA 0x20 0x06
    LDA #0x42
    STA 0x20 0x06

    LDX #0x00
    LDA 0xC0 0x81,x
:loop1
    STA 0x20 0x07
    INX
    LDA 0xC0 0x81,x
    BNE $:loop1

;
; reset scroll registers
;

    LDA #0x00
    STA 0x20 0x05
    STA 0x20 0x05

;
; Enable rendering
;

    LDA #0x88
    STA 0x20 0x00
    LDA #0x1E
    STA 0x20 0x01

:loop
    JMP $:loop
:nmi
:irq
    RTI

>

-- text, address = 0xC081
fragment ../spec/sample/uppercase_text_to_bin
<
HELLO SAPLING\0
>

