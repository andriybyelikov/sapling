--
-- This version reserves an extra byte of the carriage return in the
-- HELLO SAPLING string.
--

-- iNES Header
-- https://wiki.nesdev.com/w/index.php/INES

fragment ../spec/sample/hex_to_bin
<
    0x4E 0x45 0x53 0x1A 0x01 0x01 0x00 0x00
    0x00 0x00 0x00 0x00 0x00 0x00 0x00 0x00
>


-- NES init code
-- https://wiki.nesdev.com/w/index.php/Init_code

fragment ../spec/sample/6502_to_bin
<
    SEI          ; PC = 0xC000
    CLD          ; PC = 0xC001
    LDX #0x40    ; PC = 0xC002
    STX 64 0x17  ; PC = 0xC004
    LDX #0xFF    ; PC = 0xC007
    TXS          ; PC = 0xC009
    INX          ; PC = 0xC00A
    STX 32 0x00  ; PC = 0xC00B
    STX 32 0x01  ; PC = 0xC00E
    STX 64 0x10  ; PC = 0xC011

; vblankwait1:
    BIT 32 0x02  ; PC = 0xC014
    BPL 0xFB     ; PC = 0xC017 // diff = PC - PC_AFTER = 0xC015 - 0xC01C = 0xFB
    TXA          ; PC = 0xC019

; clrmem:
    STA 0x00,x   ; PC = 0xC01A
    STA 1 0x00,x ; PC = 0xC01C
    STA 2 0x00,x ; PC = 0xC019
    STA 3 0x00,x ; PC = 0xC022
    STA 4 0x00,x ; PC = 0xC025
    STA 5 0x00,x ; PC = 0xC028
    STA 6 0x00,x ; PC = 0xC02B
    STA 7 0x00,x ; PC = 0xC02E
    INX          ; PC = 0xC031
    BNE 0xE6     ; PC = 0xC032 // diff = PC - PC_AFTER = 0xC01B - 0xC035 = 0xE6

; vblankwait2:
    BIT 32 0x02  ; PC = 0xC034
    BPL 0xFB     ; PC = 0xC037 // diff = PC - PC_AFTER = 0xC035 - 0xC03A = 0xFB
                 ; PC = 0xC039

; end of init code


;
; Load palette
;

    ; reset $2006 flip-flop
    BIT 32 0x06  ; PC = 0xC039
    ; set $2006 flip-flop address to palette vram
    LDA #0x3F    ; PC = 0xC03C
    STA 32 0x06  ; PC = 0xC03E
    LDA #0x00    ; PC = 0xC041
    STA 32 0x06  ; PC = 0xC043

    LDA #0x02    ; PC = 0xC046
    STA 32 0x07  ; PC = 0xC048
    LDA #0x30    ; PC = 0xC04B
    STA 32 0x07  ; PC = 0xC04D

;
; Write sample text
;

    ; reset $2006 flip-flop
    BIT 32 0x06  ; PC = 0xC050
    ; set $2006 flip-flop address to name table #0 vram
    LDA #0x20    ; PC = 0xC053
    STA 32 0x06  ; PC = 0xC055
    LDA #0x42    ; PC = 0xC058
    STA 32 0x06  ; PC = 0xC05A

    LDX #0x00    ; PC = 0xC05D
; loop: // PC = 0xC05F
    LDA 192 0x81,x ; PC = 0xC05F
    BEQ 0x07     ; PC = 0xC062 // diff = PC - PC_AFTER = 0xC06C - 0xC065 = 0x07
    STA 32 0x07  ; PC = 0xC064
    INX          ; PC = 0xC067
    JMP 192 0x5F ; PC = 0xC068
; exitloop: // PC = 0xC06B

;
; reset scroll registers
;

    LDA #0x00    ; PC = 0xC06B
    STA 32 0x05  ; PC = 0xC06D
    STA 32 0x05  ; PC = 0xC070

;
; Enable rendering
;

    LDA #0x88    ; PC = 0xC073
    STA 32 0x00  ; PC = 0xC075
    LDA #0x1E    ; PC = 0xC078
    STA 32 0x01  ; PC = 0xC07A


; loop:
    JMP 192 0x7D ; PC = 0xC07D
; nmi:
; irq:
    RTI          ; PC = 0xC080
                 ; PC = 0xC081
>

-- text, address = 0xC081
fragment ../spec/sample/uppercase_text_to_bin
<
HELLO SAPLING\0
>

