t_gobble__blank -> [ \t\r\n]+;
t_mnemonic      -> [A-Z]+;

start             -> instructions;
instructions      -> instruction instructions | instruction;
instruction       -> t_mnemonic
<
    (if (strequ (lexeme "t_mnemonic") "SEI")
        (emit_byte 0x78)
    (if (strequ (lexeme "t_mnemonic") "CLD")
        (emit_byte 0xD8)
        (nil)
    ))
>;
